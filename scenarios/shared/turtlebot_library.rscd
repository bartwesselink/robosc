library TurtlebotLibrary {
    datatype object Vector3 {
        x: double
        y: double
        z: double
    }

    datatype object Twist {
        linear: Vector3
        angular: Vector3
    }

    component TurtlebotPlatform {
        incoming message move with identifier: "/cmd_vel", type: Twist (import Twist from geometry_msgs)
        incoming message halt with identifier: "/cmd_vel", type: Twist (import Twist from geometry_msgs)
    }
}